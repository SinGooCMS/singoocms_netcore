@{
    Layout = "~/views/platform/h5/MasterPage/IFrame.cshtml"; //母版页
}

<form method="post" singoo-data-ajax="true" singoo-data-dialog="true" action="/platform/DictItem/@(ViewBag.ParamAction)">
    <script type="text/html" id="singoo-template">
        {{#result}}
        <div class="form-box">
            <ul>
                <li class="form-item clear-fix">
                    <div class="form-item-title">
                        当前字典：
                    </div>
                    <div class="form-item-con text-item focus-item">
                        @(ViewBag.Dict.DictDesc)
                    </div>
                </li>
                <li class="form-item clear-fix">
                    <div class="form-item-title">
                        字典键：
                    </div>
                    <div class="form-item-con text-item focus-item">
                        <input name="TextBox1" type="text" value="{{KeyName}}" maxlength="50" id="TextBox1" placeholder="请输入字典内容键(英文、数字) 如 BeiJing" required="required" pattern="\w+" />
                    </div>
                </li>
                <li class="form-item clear-fix">
                    <div class="form-item-title">
                        字典值：
                    </div>
                    <div class="form-item-con text-item focus-item">
                        <input name="TextBox2" type="text" value="{{KeyValue}}" maxlength="100" id="TextBox2" placeholder="请输入字典内容值 如 北京" required="required" />
                    </div>
                </li>
                <li class="btn-box">
                    <input type="submit" name="btnok" value="确 认" id="btnok" class="btn-style sure-btn" />
                    <input id="btncancel" singoo-dialog-close="true" type="button" value="取 消(ESC)" class="btn-style cancel-btn" />
                    <input type="hidden" name="_dictid" value="@(ViewBag.Dict.AutoID)" />
                </li>
            </ul>
        </div>
        {{/result}}
    </script>
</form>

<script>
    var json = @Html.Raw(ViewBag.InitData); //数据
    var tmpl=$("#singoo-template").html(); //模板
    h5.render($("form"),tmpl,json); //渲染并显示模板
</script>